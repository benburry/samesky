{
  "name": "To Me To You",
  "version": "0.01",
  "manifest_version": 2,
  "minimum_chrome_version": "10.0",

  "homepage_url": "https://github.com/benburry/samesky",

  "icons": {
    "16": "icons/mrbenn_16.png",
    "48": "icons/mrbenn_48.png",
    "128": "icons/mrbenn_128.png"
  },

  "permissions": [
    "storage"
  ],

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }

}
